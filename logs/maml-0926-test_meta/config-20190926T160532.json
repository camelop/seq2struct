{
    "data": {
        "train": {
            "db_path": "data/spider-20190205/database",
            "name": "spider",
            "paths": [
                "data/spider-20190205/train_spider.json",
                "data/spider-20190205/train_others.json"
            ],
            "tables_paths": [
                "data/spider-20190205/tables.json"
            ]
        },
        "val": {
            "db_path": "data/spider-20190205/database",
            "name": "spider",
            "paths": [
                "data/spider-20190205/dev.json"
            ],
            "tables_paths": [
                "data/spider-20190205/tables.json"
            ]
        }
    },
    "lr_scheduler": {
        "decay_steps": 38000,
        "end_lr": 0,
        "name": "warmup_polynomial",
        "num_warmup_steps": 2000,
        "power": 0.5,
        "start_lr": 0.001
    },
    "meta_learning": {
        "to_do": "TODO HERE"
    },
    "model": {
        "decoder": {
            "desc_attn": "mha",
            "dropout": 0.2,
            "name": "NL2Code"
        },
        "decoder_preproc": {
            "grammar": {
                "include_literals": false,
                "name": "spider",
                "output_from": false,
                "use_table_pointer": false
            },
            "max_count": 5000,
            "min_freq": 50,
            "save_path": "data/spider-20190205/nl2code-0401,output_from=false,emb=glove-42B,min_freq=50/",
            "use_seq_elem_rules": true
        },
        "encoder": {
            "batch_encs_update": false,
            "column_encoder": [
                "emb",
                "bilstm-summarize"
            ],
            "dropout": 0.2,
            "name": "spiderv2",
            "question_encoder": [
                "emb",
                "bilstm"
            ],
            "table_encoder": [
                "emb",
                "bilstm-summarize"
            ],
            "update_config": {
                "name": "relational_transformer",
                "num_heads": 8,
                "num_layers": 4
            },
            "word_emb_size": 300
        },
        "encoder_preproc": {
            "count_tokens_in_word_emb_for_vocab": false,
            "include_table_name_in_column": false,
            "max_count": 5000,
            "min_freq": 50,
            "save_path": "data/spider-20190205/nl2code-0401,output_from=false,emb=glove-42B,min_freq=50/",
            "word_emb": {
                "kind": "42B",
                "name": "glove"
            }
        },
        "name": "EncDec"
    },
    "optimizer": {
        "lr": 0,
        "name": "adam"
    },
    "train": {
        "batch_size": 50,
        "enable_meta_learning": true,
        "eval_batch_size": 50,
        "eval_every_n": 100,
        "keep_every_n": 1000,
        "max_steps": 40000,
        "num_eval_items": 50,
        "report_every_n": 10,
        "save_every_n": 100
    }
}